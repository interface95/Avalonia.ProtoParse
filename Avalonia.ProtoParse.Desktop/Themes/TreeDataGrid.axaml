<Styles xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:conv="using:Avalonia.Controls.Converters"
        xmlns:localConv="using:Avalonia.ProtoParse.Desktop.Converters"
        xmlns:core="clr-namespace:Avalonia.ProtoParse.Desktop.Core"
        xmlns:vm="using:Avalonia.ProtoParse.Desktop.ViewModels">

    <Styles.Resources>
        <ResourceDictionary>
            <localConv:ParentNodeBackgroundConverter x:Key="ParentNodeBackgroundConverter" />
            <localConv:ParentNodeBorderBrushConverter x:Key="ParentNodeBorderBrushConverter" />
            <localConv:ParentNodeBorderThicknessConverter x:Key="ParentNodeBorderThicknessConverter" />
            <localConv:HasChildrenConverter x:Key="HasChildrenConverter" />

            <localConv:WireTypeToBrushConverter x:Key="WireTypeToBrushConverter" />
            <localConv:WireTypeToBackgroundConverter x:Key="WireTypeToBackgroundConverter" />
            <localConv:WireTypeToBorderBrushConverter x:Key="WireTypeToBorderBrushConverter" />

            <DataTemplate x:Key="ProtoNodeTemplate" DataType="core:ProtoDisplayNode">
                <!-- Use Grid for layering -->
                <Grid Margin="0,2,16,2">
                    <!-- Layer 1: Dynamic Background -->
                    <Border CornerRadius="6"
                            Background="{Binding CardBackgroundBrush}"
                            IsHitTestVisible="False" />

                    <!-- Layer 2: Active State (Selection/Hover) - Animated Opacity -->
                    <Border Name="ActiveStateBorder"
                            CornerRadius="6"
                            IsHitTestVisible="False"
                            Classes.selected="{Binding IsSelected, RelativeSource={RelativeSource AncestorType=TreeDataGridRow}}"
                            Classes.hovered="{Binding IsPointerOver, RelativeSource={RelativeSource AncestorType=TreeDataGridRow}}">
                        <Border.Transitions>
                            <Transitions>
                                <DoubleTransition Property="Opacity" Duration="0:0:0.15" Easing="CircularEaseOut"/>
                            </Transitions>
                        </Border.Transitions>
                    </Border>

                    <!-- Layer 3: WireType Border (Persistent) -->
                    <Border CornerRadius="6"
                            BorderBrush="{Binding CardBorderBrush}"
                            BorderThickness="{Binding CardBorderThickness}"
                            IsHitTestVisible="False" />

                    <!-- Layer 4: Content -->
                    <Border Name="ContentContainer" CornerRadius="6" Padding="6,6">
                        <StackPanel Orientation="Horizontal" Spacing="8" VerticalAlignment="Center">
                            <!-- Custom Expander Button -->
                            <ToggleButton Classes="NodeExpander"
                                          IsChecked="{Binding IsExpanded, Mode=OneWay}"
                                          IsVisible="{Binding HasChildren}"
                                          x:CompileBindings="False"
                                          Command="{Binding $parent[UserControl].DataContext.ToggleNodeCommand}"
                                          CommandParameter="{Binding}"
                                          VerticalAlignment="Center" />
    
                            <!-- Badge -->
                            <Border MinWidth="24"
                                    Height="24"
                                    CornerRadius="12"
                                    Padding="8,0"
                                    VerticalAlignment="Center"
                                    Background="{Binding BadgeBackgroundBrush}">
                                <TextBlock Text="{Binding FieldDisplay}"
                                           HorizontalAlignment="Center"
                                           VerticalAlignment="Center"
                                           FontWeight="Bold"
                                           FontSize="11"
                                           Foreground="{Binding BadgeBrush}" />
                            </Border>
    
                            <!-- Text Content -->
                            <StackPanel VerticalAlignment="Center" Spacing="2">
                                <TextBlock Text="{Binding WireTypeDisplay}"
                                           Foreground="{Binding BadgeBrush}"
                                           FontWeight="Bold"
                                           FontSize="12" />
                                <TextBlock Text="{Binding Summary}"
                                           FontSize="11"
                                           Foreground="{DynamicResource TextSecondary}"
                                           TextTrimming="CharacterEllipsis"
                                           Opacity="0.9"
                                           VerticalAlignment="Center"/>
                            </StackPanel>
                        </StackPanel>
                    </Border>
                </Grid>
            </DataTemplate>

            <StreamGeometry x:Key="TreeDataGridSortIconDescendingPath">M1875 1011l-787 787v-1798h-128v1798l-787 -787l-90 90l941 941l941 -941z</StreamGeometry>
            <StreamGeometry x:Key="TreeDataGridSortIconAscendingPath">M1965 947l-941 -941l-941 941l90 90l787 -787v1798h128v-1798l787 787z</StreamGeometry>
            <StreamGeometry x:Key="TreeDataGridItemCollapsedChevronPathData">M 1,0 10,10 l -9,10 -1,-1 L 8,10 -0,1 Z</StreamGeometry>
            <StreamGeometry x:Key="TreeDataGridItemExpandedChevronPathData">M0,1 L10,10 20,1 19,0 10,8 1,0 Z</StreamGeometry>
            
            <!-- ... (Themes omitted for brevity) ... -->



            <ControlTheme x:Key="{x:Type TreeDataGrid}" TargetType="TreeDataGrid">
                <Setter Property="Template">
                    <ControlTemplate>
                        <Border x:Name="RootBorder"
                                Background="{TemplateBinding Background}"
                                BorderBrush="{TemplateBinding BorderBrush}"
                                BorderThickness="{TemplateBinding BorderThickness}"
                                CornerRadius="{TemplateBinding CornerRadius}">
                            <DockPanel>
                                <ScrollViewer Name="PART_HeaderScrollViewer"
                                              DockPanel.Dock="Top"
                                              IsVisible="{TemplateBinding ShowColumnHeaders}"
                                              HorizontalScrollBarVisibility="Hidden"
                                              VerticalScrollBarVisibility="Disabled"
                                              BringIntoViewOnFocusChange="{TemplateBinding (ScrollViewer.BringIntoViewOnFocusChange)}">
                                    <Border x:Name="ColumnHeadersPresenterBorder">
                                        <TreeDataGridColumnHeadersPresenter Name="PART_ColumnHeadersPresenter"
                                                                            ElementFactory="{TemplateBinding ElementFactory}"
                                                                            Items="{TemplateBinding Columns}" />
                                    </Border>
                                </ScrollViewer>
                                <ScrollViewer Name="PART_ScrollViewer"
                                              HorizontalScrollBarVisibility="Auto"
                                              VerticalScrollBarVisibility="Auto"
                                              BringIntoViewOnFocusChange="{TemplateBinding (ScrollViewer.BringIntoViewOnFocusChange)}">
                                    <TreeDataGridRowsPresenter Name="PART_RowsPresenter"
                                                               Columns="{TemplateBinding Columns}"
                                                               ElementFactory="{TemplateBinding ElementFactory}"
                                                               Items="{TemplateBinding Rows}" />
                                </ScrollViewer>
                            </DockPanel>
                        </Border>
                    </ControlTemplate>
                </Setter>

                <Style Selector="^/template/ Border#ColumnHeadersPresenterBorder">
                    <Setter Property="BorderThickness" Value="0 0 0 1" />
                    <Setter Property="BorderBrush" Value="{DynamicResource TreeDataGridGridLinesBrush}" />
                </Style>

            </ControlTheme>

            <ControlTheme x:Key="{x:Type TreeDataGridColumnHeader}" TargetType="TreeDataGridColumnHeader">
                <Setter Property="Background" Value="Transparent" />
                <Setter Property="MinHeight" Value="25" />
                <Setter Property="Padding" Value="4 2" />
                <Setter Property="VerticalContentAlignment" Value="Center" />
                <Setter Property="Template">
                    <ControlTemplate>
                        <Border Name="DataGridBorder"
                                Background="{TemplateBinding Background}"
                                BorderBrush="{TemplateBinding BorderBrush}"
                                BorderThickness="{TemplateBinding BorderThickness}"
                                CornerRadius="{TemplateBinding CornerRadius}">
                            <DockPanel VerticalAlignment="Stretch">
                                <Panel DockPanel.Dock="Right"
                                       TabIndex="2">
                                    <Rectangle Fill="{DynamicResource TreeDataGridGridLinesBrush}"
                                               HorizontalAlignment="Right"
                                               Width="1" />
                                    <Thumb Name="PART_Resizer"
                                           DockPanel.Dock="Right"
                                           Background="Transparent"
                                           Cursor="SizeWestEast"
                                           IsVisible="{TemplateBinding CanUserResize}"
                                           Width="5">
                                        <Thumb.Template>
                                            <ControlTemplate>
                                                <Border Background="{TemplateBinding Background}"
                                                        VerticalAlignment="Stretch" />
                                            </ControlTemplate>
                                        </Thumb.Template>
                                    </Thumb>
                                </Panel>
                                <Path Name="SortIcon"
                                      DockPanel.Dock="Right"
                                      Fill="{TemplateBinding Foreground}"
                                      HorizontalAlignment="Center"
                                      VerticalAlignment="Center"
                                      Stretch="Uniform"
                                      Height="12"
                                      TabIndex="1" />
                                <ContentPresenter Name="PART_ContentPresenter"
                                                  Content="{TemplateBinding Header}"
                                                  ContentTemplate="{TemplateBinding ContentTemplate}"
                                                  Padding="{TemplateBinding Padding}"
                                                  HorizontalContentAlignment="{TemplateBinding HorizontalContentAlignment}"
                                                  VerticalContentAlignment="{TemplateBinding VerticalContentAlignment}"
                                                  TabIndex="0">
                                    <ContentPresenter.DataTemplates>
                                        <DataTemplate DataType="x:String">
                                            <TextBlock Text="{Binding}" TextTrimming="CharacterEllipsis" />
                                        </DataTemplate>
                                    </ContentPresenter.DataTemplates>
                                </ContentPresenter>
                            </DockPanel>
                        </Border>
                    </ControlTemplate>
                </Setter>

                <Style Selector="^:pointerover /template/ Border#DataGridBorder">
                    <Setter Property="Background"
                            Value="{DynamicResource TreeDataGridHeaderBackgroundPointerOverBrush}" />
                    <Setter Property="BorderBrush"
                            Value="{DynamicResource TreeDataGridHeaderBorderBrushPointerOverBrush}" />
                    <Setter Property="TextBlock.Foreground"
                            Value="{DynamicResource TreeDataGridHeaderForegroundPointerOverBrush}" />
                </Style>

                <Style Selector="^:pressed /template/ Border#DataGridBorder">
                    <Setter Property="Background" Value="{DynamicResource TreeDataGridHeaderBackgroundPressedBrush}" />
                    <Setter Property="BorderBrush" Value="{DynamicResource TreeDataGridHeaderBorderBrushPressedBrush}" />
                    <Setter Property="TextBlock.Foreground"
                            Value="{DynamicResource TreeDataGridHeaderForegroundPressedBrush}" />
                </Style>

                <Style Selector="^[SortDirection=Ascending] /template/ Path#SortIcon">
                    <Setter Property="IsVisible" Value="True" />
                    <Setter Property="Data" Value="{DynamicResource TreeDataGridSortIconAscendingPath}" />
                </Style>

                <Style Selector="^[SortDirection=Descending] /template/ Path#SortIcon">
                    <Setter Property="IsVisible" Value="True" />
                    <Setter Property="Data" Value="{DynamicResource TreeDataGridSortIconDescendingPath}" />
                </Style>

            </ControlTheme>

            <!-- TreeDataGridRow 样式 -->
            <ControlTheme x:Key="{x:Type TreeDataGridRow}" TargetType="TreeDataGridRow">
                <Setter Property="Template">
                    <ControlTemplate>
                        <Border x:Name="RowBorder"
                                Background="{TemplateBinding Background}"
                                BorderBrush="{TemplateBinding BorderBrush}"
                                BorderThickness="{TemplateBinding BorderThickness}"
                                CornerRadius="{TemplateBinding CornerRadius}">
                            <TreeDataGridCellsPresenter Name="PART_CellsPresenter"
                                                        ElementFactory="{TemplateBinding ElementFactory}"
                                                        Items="{TemplateBinding Columns}"
                                                        Rows="{TemplateBinding Rows}" />
                        </Border>
                    </ControlTemplate>
                </Setter>

                <!-- 选中时，移除 CellsPresenter 的背景 -->
                <Style Selector="^:selected /template/ TreeDataGridCellsPresenter#PART_CellsPresenter">
                    <Setter Property="Background" Value="Transparent" />
                </Style>
                <!-- 悬停时，移除 CellsPresenter 的背景 -->
                <Style Selector="^:pointerover /template/ TreeDataGridCellsPresenter#PART_CellsPresenter">
                    <Setter Property="Background" Value="Transparent" />
                </Style>
            </ControlTheme>

            <!-- 展开/折叠箭头样式 (保留，虽然被隐藏但可能被引用) -->
            <ControlTheme x:Key="TreeDataGridExpandCollapseChevron" TargetType="ToggleButton">
                <Setter Property="Margin" Value="0" />
                <Setter Property="Width" Value="18" />
                <Setter Property="Height" Value="18" />
                <Setter Property="Template">
                    <ControlTemplate>
                        <Border Background="{DynamicResource ExpanderBackground}"
                                BorderBrush="{DynamicResource ExpanderBorder}"
                                BorderThickness="1"
                                CornerRadius="4"
                                Width="{TemplateBinding Width}"
                                Height="{TemplateBinding Height}"
                                HorizontalAlignment="Center"
                                VerticalAlignment="Center">
                            <Viewbox Width="12" Height="12"
                                     HorizontalAlignment="Center"
                                     VerticalAlignment="Center">
                                <Path x:Name="ChevronPath"
                                      Data="{StaticResource TreeDataGridItemCollapsedChevronPathData}"
                                      Fill="{DynamicResource ExpanderForeground}"
                                      Stretch="Uniform" />
                            </Viewbox>
                        </Border>
                    </ControlTemplate>
                </Setter>
                <Style Selector="^:checked /template/ Path#ChevronPath">
                    <Setter Property="Data" Value="{StaticResource TreeDataGridItemExpandedChevronPathData}" />
                </Style>
            </ControlTheme>
        </ResourceDictionary>
    </Styles.Resources>

    <!-- 单元格通用样式 -->
    <Style Selector=":is(TreeDataGridCell)">
        <Setter Property="Background" Value="Transparent" />
        <Setter Property="MinHeight" Value="25" />
    </Style>

    <!-- 单元格选中样式 -->
    <Style Selector=":is(TreeDataGridCell):selected">
        <Setter Property="Background" Value="{DynamicResource TreeDataGridSelectedCellBackgroundBrush}" />
    </Style>

    <!-- 从 App.axaml 移动过来的样式 -->
    <Style Selector="Border.highlighted Border.field-badge">
        <Setter Property="Background" Value="Transparent" />
    </Style>

    <!-- 
        ProtoTheme for TreeDataGrid 
        Features:
        - Removes default system selection colors (blue background)
        - Hides default expander arrows
        - Supports custom internal expander buttons (.NodeExpander)
        - Implements card-style selection visuals (borders instead of backgrounds)
    -->

    <Style Selector="TreeDataGrid.ProtoTheme">
        <Style.Resources>
            <!-- Force transparency on ALL possible selection background brushes to kill the blue highlight -->
            <SolidColorBrush x:Key="SystemControlHighlightListAccentLowBrush" Color="Transparent"/>
            <SolidColorBrush x:Key="SystemControlHighlightListAccentMediumBrush" Color="Transparent"/>
            <SolidColorBrush x:Key="SystemControlHighlightListAccentHighBrush" Color="Transparent"/>
            <SolidColorBrush x:Key="SystemControlHighlightListLowBrush" Color="Transparent"/>
            <SolidColorBrush x:Key="SystemControlHighlightListMediumBrush" Color="Transparent"/>
            <SolidColorBrush x:Key="SystemControlHighlightListHighBrush" Color="Transparent"/>
            <SolidColorBrush x:Key="SystemControlHighlightAltListAccentLowBrush" Color="Transparent"/>
            <SolidColorBrush x:Key="SystemControlHighlightAltListAccentMediumBrush" Color="Transparent"/>
            <SolidColorBrush x:Key="SystemControlHighlightAltListAccentHighBrush" Color="Transparent"/>
            
            <SolidColorBrush x:Key="TreeDataGridRowSelectionBackground" Color="Transparent"/>
            <SolidColorBrush x:Key="TreeDataGridRowSelectionBackgroundPointerOver" Color="Transparent"/>
            <SolidColorBrush x:Key="TreeDataGridRowSelectionBackgroundPressed" Color="Transparent"/>
            <SolidColorBrush x:Key="TreeDataGridRowSelectionBackgroundSelected" Color="Transparent"/>
            <SolidColorBrush x:Key="TreeDataGridRowSelectionBackgroundSelectedPointerOver" Color="Transparent"/>
            <SolidColorBrush x:Key="TreeDataGridRowSelectionBackgroundSelectedPressed" Color="Transparent"/>
            <SolidColorBrush x:Key="TreeDataGridSelectedCellBackgroundBrush" Color="Transparent"/>
        </Style.Resources>
    </Style>

    <!-- Hide Headers -->
    <Style Selector="TreeDataGrid.ProtoTheme TreeDataGridColumnHeader">
        <Setter Property="IsVisible" Value="False"/>
        <Setter Property="MinHeight" Value="0"/>
        <Setter Property="Height" Value="0"/>
    </Style>

    <!-- High priority overrides for Row Background to ensure no blue selection color remains -->
    <!-- High priority overrides for Row Background to ensure no blue selection color remains -->
    <Style Selector="TreeDataGrid.ProtoTheme TreeDataGridRow:selected /template/ Border#RowBorder">
        <Setter Property="Background" Value="{x:Null}"/>
        <Setter Property="BorderBrush" Value="Transparent"/>
        <Setter Property="BorderThickness" Value="0"/>
    </Style>
    <Style Selector="TreeDataGrid.ProtoTheme TreeDataGridRow:pointerover /template/ Border#RowBorder">
        <Setter Property="Background" Value="{x:Null}"/>
        <Setter Property="BorderBrush" Value="Transparent"/>
        <Setter Property="BorderThickness" Value="0"/>
    </Style>
    <Style Selector="TreeDataGrid.ProtoTheme TreeDataGridRow:selected:pointerover /template/ Border#RowBorder">
        <Setter Property="Background" Value="{x:Null}"/>
        <Setter Property="BorderBrush" Value="Transparent"/>
        <Setter Property="BorderThickness" Value="0"/>
    </Style>

    <!-- Force Cells to be transparent so only the Card is visible -->
    <Style Selector="TreeDataGrid.ProtoTheme :is(TreeDataGridCell):selected">
        <Setter Property="Background" Value="Transparent"/>
        <Setter Property="BorderBrush" Value="Transparent"/>
        <Setter Property="BorderThickness" Value="0"/>
    </Style>
    <Style Selector="TreeDataGrid.ProtoTheme :is(TreeDataGridCell):pointerover">
        <Setter Property="Background" Value="Transparent"/>
        <Setter Property="BorderBrush" Value="Transparent"/>
        <Setter Property="BorderThickness" Value="0"/>
    </Style>
    <Style Selector="TreeDataGrid.ProtoTheme :is(TreeDataGridCell)">
        <Setter Property="Background" Value="Transparent"/>
        <Setter Property="BorderBrush" Value="Transparent"/>
        <Setter Property="BorderThickness" Value="0"/>
    </Style>

    <!-- Specific override for ExpanderCell to beat default theme specificity -->
    <Style Selector="TreeDataGrid.ProtoTheme TreeDataGridExpanderCell:selected">
        <Setter Property="Background" Value="Transparent"/>
        <Setter Property="BorderBrush" Value="Transparent"/>
        <Setter Property="BorderThickness" Value="0"/>
    </Style>
    <Style Selector="TreeDataGrid.ProtoTheme TreeDataGridExpanderCell:pointerover">
        <Setter Property="Background" Value="Transparent"/>
        <Setter Property="BorderBrush" Value="Transparent"/>
        <Setter Property="BorderThickness" Value="0"/>
    </Style>

    <!-- ================================================================== -->
    <!-- Expander Styles -->
    <!-- ================================================================== -->

    <!-- Hide Native Expander Button -->
    <!-- This selector matches ALL ToggleButtons inside the expander cell by default -->
    <Style Selector="TreeDataGrid.ProtoTheme TreeDataGridExpanderCell ToggleButton">
        <Setter Property="IsVisible" Value="False"/>
        <Setter Property="Width" Value="0"/>
        <Setter Property="Template">
            <ControlTemplate/>
        </Setter>
    </Style>

    <!-- Restore Custom Internal Node Expander -->
    <!-- This specific style overrides the generic one above -->
    <Style Selector="TreeDataGrid.ProtoTheme ToggleButton.NodeExpander">
        <Setter Property="Width" Value="20"/>
        <Setter Property="Height" Value="20"/>
        <Setter Property="VerticalAlignment" Value="Center"/>
        <Setter Property="Background" Value="Transparent"/>
        <Setter Property="Template">
            <ControlTemplate>
                    <Border Width="18" Height="18" CornerRadius="4" BorderThickness="1" 
                            BorderBrush="{DynamicResource ProtoExpanderBorder}" 
                            Background="{DynamicResource ProtoExpanderBackground}" 
                            HorizontalAlignment="Center" VerticalAlignment="Center">
                        <Path Name="Arrow" 
                            Data="M 6 5 L 10 9 L 6 13" 
                            Stroke="{DynamicResource ProtoExpanderStroke}" 
                            StrokeThickness="2"
                            StrokeLineCap="Round"
                            HorizontalAlignment="Center" 
                            VerticalAlignment="Center"
                            Width="6" Height="8" Stretch="Uniform"
                            RenderTransformOrigin="50%,50%">
                        <Path.Transitions>
                            <Transitions>
                                <TransformOperationsTransition Property="RenderTransform" Duration="0:0:0.1"/>
                            </Transitions>
                        </Path.Transitions>
                        </Path>
                    </Border>
            </ControlTemplate>
        </Setter>
    </Style>
    
    <!-- Interaction States for Our Node Expander -->
    <Style Selector="TreeDataGrid.ProtoTheme ToggleButton.NodeExpander:checked /template/ Path#Arrow">
            <Setter Property="RenderTransform" Value="rotate(90deg)"/>
            <Setter Property="Stroke" Value="{DynamicResource ProtoExpanderStrokeChecked}"/>
    </Style>
    <Style Selector="TreeDataGrid.ProtoTheme ToggleButton.NodeExpander:pointerover /template/ Border">
            <Setter Property="Background" Value="{DynamicResource ProtoExpanderBackgroundHover}"/>
            <Setter Property="BorderBrush" Value="{DynamicResource ProtoExpanderBorderHover}"/>
    </Style>



    <!-- ================================================================== -->
    <!-- Card Visuals (Selection & Hover) -->
    <!-- ================================================================== -->

    <!-- Active State (Selection/Hover) Styles -->
    
    <!-- Base Style for ActiveStateBorder -->
    <Style Selector="Border#ActiveStateBorder">
        <Setter Property="BorderThickness" Value="1"/>
        <Setter Property="Opacity" Value="0"/>
    </Style>

    <!-- Selected State -->
    <Style Selector="Border#ActiveStateBorder.selected">
        <Setter Property="Opacity" Value="1"/>
        <Setter Property="Background" Value="{DynamicResource ProtoCardSelectedBackground}"/>
        <Setter Property="BorderBrush" Value="{DynamicResource ProtoCardSelectedBorder}"/>
    </Style>
    
    <!-- Hover State -->
    <Style Selector="Border#ActiveStateBorder.hovered">
        <Setter Property="Opacity" Value="1"/>
        <Setter Property="Background" Value="{DynamicResource ProtoCardHoverBackground}"/> 
        <Setter Property="BorderBrush" Value="{DynamicResource ProtoCardSelectedBorder}"/>
    </Style>

    <!-- ================================================================== -->
    <!-- WireType Text Colors -->
    <!-- ================================================================== -->
    
    <Style Selector="TextBlock.WireType">
        <Setter Property="Foreground" Value="{DynamicResource WireTypeDefaultBrush}"/>
    </Style>
    
    <Style Selector="TextBlock.Varint">
        <Setter Property="Foreground" Value="{DynamicResource WireTypeVarintBrush}"/>
    </Style>
    
    <Style Selector="TextBlock.LengthDelimited">
        <Setter Property="Foreground" Value="{DynamicResource WireTypeLengthDelimitedBrush}"/>
    </Style>
    
    <Style Selector="TextBlock.Fixed32">
        <Setter Property="Foreground" Value="{DynamicResource WireTypeFixedBrush}"/>
    </Style>
    
    <Style Selector="TextBlock.Fixed64">
        <Setter Property="Foreground" Value="{DynamicResource WireTypeFixedBrush}"/>
    </Style>
</Styles>